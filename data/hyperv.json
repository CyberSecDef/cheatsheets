						"headers": ["Control", "PowerShell"],
						"rows": [
							["CPU reserve/limit/weight", "Set-VMProcessor -VMName vm01 -Reserve 10 -Maximum 75 -RelativeWeight 100"],
							["Memory buffer/priority", "Set-VMMemory -VMName vm01 -Buffer 20 -Priority 50"],
							["NUMA settings", "Get-VMHostNumaNode" ]
						]
					},
					"example": "Set-VMProcessor -VMName vm01 -Maximum 75"
