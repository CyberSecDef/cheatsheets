	"categories": [
		{
			"title": "Fundamentals and Help",
			"description": "Core concepts and how to discover commands quickly.",
			"items": [
				{
					"title": "PowerShell help system",
					"description": "PowerShell’s help is structured and discoverable; update it on older systems.",
					"table": {
						"headers": ["Command", "Description"],
						"rows": [
							["Get-Help [Cmdlet]", "Show help (use -Full/-Examples)."],
							["Get-Help [Cmdlet] -Examples", "Quick examples."],
							["Get-Command [name]", "Find cmdlets, functions, aliases, executables."],
							["Get-Command -Noun Service", "Discover cmdlets by noun."],
							["Get-Member", "Inspect object properties/methods in the pipeline."],
							["Update-Help", "Download newest help (admin often required)." ]
						]
					},
					"example": "Get-Help Get-Service -Examples\nGet-Command *process*\nGet-Service | Get-Member"
				},
				{
					"title": "CMD vs PowerShell (quick mental model)",
					"description": "CMD pipelines are mostly text; PowerShell pipelines pass objects.",
					"table": {
						"headers": ["Topic", "CMD", "PowerShell"],
						"rows": [
							["Pipeline", "Text", "Objects"],
							["Help", "command /?", "Get-Help"],
							["Common list", "dir", "Get-ChildItem (alias: dir/ls)"],
							["Admin shell", "Run as Administrator", "Run as Administrator"],
							["Scripting", ".bat/.cmd", ".ps1 (+ modules)" ]
						]
					},
					"example": "# In PowerShell, this is object-based:\nGet-Process | Sort-Object CPU -Descending | Select-Object -First 5"
				},
				{
					"title": "System identity",
					"description": null,
					"table": {
						"headers": ["Need", "Command"],
						"rows": [
							["OS version", "Get-ComputerInfo | Select-Object WindowsProductName,WindowsVersion,OsHardwareAbstractionLayer"],
							["Hostname", "$env:COMPUTERNAME"],
							["Who am I?", "whoami"],
							["PowerShell version", "$PSVersionTable" ]
						]
					},
					"example": "$PSVersionTable\nwhoami"
				}
			]
		},
		{
			"title": "Filesystem, Paths, and Environment",
			"description": "Drives, paths, file operations, and common environment variables.",
			"items": [
				{
					"title": "Common Windows paths",
					"description": "Useful locations to know when troubleshooting.",
					"table": {
						"headers": ["Path", "Purpose"],
						"rows": [
							["C:\\Windows", "Windows OS files."],
							["C:\\Windows\\System32", "System binaries and DLLs."],
							["C:\\Program Files", "64-bit apps."],
							["C:\\Program Files (x86)", "32-bit apps."],
							["C:\\Users\\[user]", "User profile (home)."],
							["%APPDATA%", "Roaming user app data."],
							["%LOCALAPPDATA%", "Local user app data."],
							["%TEMP%", "Temporary directory." ]
						]
					},
					"example": "$env:USERPROFILE\n$env:APPDATA\n$env:TEMP"
				},
				{
					"title": "File operations (PowerShell)",
					"description": "Prefer cmdlets for scripting; they are consistent and object-friendly.",
					"table": {
						"headers": ["Task", "Command"],
						"rows": [
							["List", "Get-ChildItem -Force"],
							["Current dir", "Get-Location"],
							["Change dir", "Set-Location C:\\"],
							["Create dir", "New-Item -ItemType Directory -Path .\\logs"],
							["Create file", "New-Item -ItemType File -Path .\\a.txt"],
							["Copy", "Copy-Item src dst -Recurse"],
							["Move", "Move-Item src dst"],
							["Delete", "Remove-Item path -Recurse -Force" ]
						]
					},
					"example": "Get-ChildItem -Force\nNew-Item -ItemType Directory -Path .\\demo\nRemove-Item .\\demo -Recurse -Force"
				},
				{
					"title": "Search for files and content",
					"description": "Use Get-ChildItem for filesystem, Select-String for content.",
					"table": {
						"headers": ["Goal", "Command"],
						"rows": [
							["Find files by name", "Get-ChildItem -Recurse -Filter '*.log' -ErrorAction SilentlyContinue"],
							["Search inside files", "Select-String -Path .\\*.log -Pattern 'ERROR'"],
							["Search with regex", "Select-String -Path .\\*.txt -Pattern '^WARN'" ]
						]
					},
					"example": "Select-String -Path C:\\Windows\\Logs\\*.log -Pattern 'fail' -ErrorAction SilentlyContinue"
				}
			]
		},
		{
			"title": "Processes, Services, and Scheduled Tasks",
			"description": "Inspect what’s running and control service state.",
			"items": [
				{
					"title": "Process management",
					"description": null,
					"table": {
						"headers": ["Task", "Command"],
						"rows": [
							["List processes", "Get-Process"],
							["Top CPU", "Get-Process | Sort-Object CPU -Descending | Select-Object -First 10"],
							["Find by name", "Get-Process -Name notepad -ErrorAction SilentlyContinue"],
							["Kill process", "Stop-Process -Id 1234 -Force"],
							["Show process owner", "Get-CimInstance Win32_Process | Select Name,ProcessId" ]
						]
					},
					"example": "Get-Process | Sort-Object WorkingSet64 -Descending | Select-Object -First 5 Name,Id,@{n='MB';e={[math]::Round($_.WorkingSet64/1MB,1)}}"
				},
				{
					"title": "Service management",
					"description": "Requires admin for many services.",
					"table": {
						"headers": ["Task", "Command"],
						"rows": [
							["List services", "Get-Service"],
							["Find service", "Get-Service | Where-Object Name -like '*ssh*'"],
							["Start/stop/restart", "Start-Service; Stop-Service; Restart-Service"],
							["Startup type", "Get-Service | Select Name,Status,StartType"],
							["Legacy", "sc.exe query; sc.exe qc [service]" ]
						]
					},
					"example": "Get-Service | Where-Object Status -eq 'Running' | Select-Object -First 10"
				},
				{
					"title": "Scheduled tasks",
					"description": "Windows Task Scheduler is the native job runner.",
					"table": {
						"headers": ["Task", "Command"],
						"rows": [
							["List tasks", "Get-ScheduledTask"],
							["Get task info", "Get-ScheduledTask -TaskName '...'; Get-ScheduledTaskInfo -TaskName '...'"],
							["Run a task", "Start-ScheduledTask -TaskName '...'"],
							["Legacy CLI", "schtasks /Query /FO LIST /V" ]
						]
					},
					"example": "schtasks /Query /FO LIST /V | more"
				}
			]
		},
